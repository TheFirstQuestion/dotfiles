"*":
  core:
    audioBeep: false
    autoHideMenuBar: true
    closeDeletedFileTabs: true
    customFileTypes:
      "source.git-config": [
        "gitignore"
      ]
      "source.shell": [
        "rc"
      ]
    disabledPackages: [
      "language-conky"
      "tree-view-sort"
      "MagicPython"
      "atom-ide-terminal"
      "tree-view-filter"
      "dbex"
      "atom-dbex-mysql"
      "Hydrogen"
      "language-gfm"
    ]
    restorePreviousWindowsOnStart: "no"
    telemetryConsent: "no"
  editor:
    fontFamily: "MesloLGS NF, Menlo, Consolas, DejaVu Sans Mono, monospace"
    fontSize: 19
    scrollPastEnd: true
    showIndentGuide: true
    softWrap: true
    softWrapHangingIndent: 2
    tabLength: 4
    zoomFontWhenCtrlScrolling: false
  "exception-reporting":
    userId: "982a6352-4454-40e9-95ca-3d5efd0b1d48"
  "ftp-remote-edit":
    config: ""
    password: "305e6ddc47af13e5c25d55b364"
  "markdown-preview":
    useGitHubStyle: true
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
      showInStatusBar: true
    useEslint: true
  "tree-view": {}
  "tree-view-filter":
    caseInsensitive: true
    globalFilter: "!*.pdf !.DS_Store !node_modules/"
    liveUpdate: true
  welcome:
    showOnStartup: false
